{
  "name": "@switchboard-xyz/common",
  "version": "2.4.5",
  "description": "common type definitions and utils for interacting with switchboard",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./esm-utils": {
      "types": "./esm-utils.d.ts",
      "import": "./esm-utils.js",
      "require": "./esm-utils.cjs"
    },
    "./protos": {
      "types": "./protos.d.ts",
      "import": "./protos.js",
      "require": "./protos.cjs"
    },
    "./networks": {
      "types": "./networks.d.ts",
      "import": "./networks.js",
      "require": "./networks.cjs"
    },
    "./secrets": {
      "types": "./secrets.d.ts",
      "import": "./secrets.js",
      "require": "./secrets.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "lib/",
    "src/",
    "package.json",
    "index.cjs",
    "index.js",
    "index.d.ts",
    "esm-utils.cjs",
    "esm-utils.js",
    "esm-utils.d.ts",
    "protos.cjs",
    "protos.js",
    "protos.d.ts",
    "networks.cjs",
    "networks.js",
    "networks.d.ts",
    "secrets.cjs",
    "secrets.js",
    "secrets.d.ts"
  ],
  "scripts": {
    "build": "pnpm exec rimraf lib lib-cjs && node ./compile-protos.js && echo '!src/esm-utils.js' >> .gitignore",
    "clean": "pnpm exec rimraf node_modules lib .turbo",
    "fix": "pnpm exec eslint --fix src/**/*.ts",
    "lint": "pnpm exec eslint src/**/*.ts",
    "prepack": "pnpm build",
    "test": "pnpm exec jest",
    "test:bun": "bun exec jest"
  },
  "dependencies": {
    "@solana/web3.js": "^1.93.0",
    "axios": "^1.7.2",
    "big.js": "^6.2.1",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0",
    "cron-validator": "^1.3.1",
    "decimal.js": "^10.4.3",
    "lodash": "^4.17.21",
    "protobufjs": "^7.2.6"
  },
  "devDependencies": {
    "@switchboard-xyz/eslint-config": "workspace:latest",
    "@types/big.js": "^6.1.6",
    "@types/bn.js": "^5.1.5",
    "@types/chai": "^4.3.4",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.195",
    "@types/shelljs": "^0.8.12",
    "chai": "^4.3.7",
    "jest": "^29.7.0",
    "protobufjs-cli": "^1.1.2",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=12"
  }
}
